import math
import os
import random
import re
import sys
while (1):
    n=int(input().strip())
    if n%2!=0:
        print("weired")
    elif n%2==0 and n>2 and n<5:
        print("not weired")
    elif n%2==0 and n>=6 and n<=20:
        print("not weired")

    elif n%2==0:
        print("not weired")
